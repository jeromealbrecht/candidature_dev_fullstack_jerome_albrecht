{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      },
      {
        "src": "/api/generer_tour_magique",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/generer_tour_magique",
        "dest": "/api/generer_tour_magique"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Link": "</static/styles.css>; rel=preload; as=style"
        }
      }
    ]
  }
  